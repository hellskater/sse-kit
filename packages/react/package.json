{
  "name": "@sse-toolkit/react",
  "version": "0.0.0",
  "license": "MIT",
  "type": "module",
  "description": "React hooks for SSE",
  "keywords": [
    "react",
    "hooks",
    "sse",
    "server-sent-events"
  ],
  "repository": "https://github.com/hellskater/sse-toolkit",
  "author": "K Srinivas Rao (https://twitter.com/Srinu53168)",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "typesVersions": {
    "*": {
      "*": [
        "dist/*"
      ]
    }
  },
  "scripts": {
    "lint": "eslint \"**/*.{ts,tsx}\" --max-warnings 0",
    "clean": "git clean -xdf dist node_modules",
    "build": "tsup",
    "test": "vitest run",
    "test:watch": "vitest",
    "typecheck": "tsc --noEmit",
    "dev": "tsup --watch"
  },
  "dependencies": {},
  "peerDependencies": {
    "react": "^17.0.2 || ^18.0.0",
    "zod": "^3.21.4"
  },
  "devDependencies": {
    "@types/node": "18.16.0",
    "@types/react": "18.2.8",
    "@types/react-dom": "18.2.4",
    "@sse-toolkit/eslint-config": "workspace:*",
    "@sse-toolkit/tsconfig": "workspace:*",
    "@sse-toolkit/tsup-config": "workspace:*",
    "eslint": "^8.47.0",
    "react": "18.2.0",
    "tsup": "6.7.0",
    "typescript": "^5.1.6",
    "zod": "^3.21.4",
    "vitest": "^0.30.1"
  },
  "eslintConfig": {
    "root": true,
    "extends": [
      "@sse-toolkit/eslint-config/base",
      "@sse-toolkit/eslint-config/react"
    ]
  },
  "prettier": {
    "root": true
  }
}
